<?xml version="1.0"?>
<chroma>
<annotation>
Mres calculation
</annotation>
<Param> 
  <InlineMeasurements>

    <elem>
      <Name>MAKE_SOURCE</Name>
      <Frequency>1</Frequency>
      <Param>
        <version>5</version>
        <wave_state>S_WAVE</wave_state>
        <source_type>POINT_SOURCE</source_type>
        <j_decay>3</j_decay>
        <t_source>0 0 0 0</t_source>
        <nrow>4 4 4 8</nrow>
      </Param>
      <NamedObject>
        <source_id>sh_source_0</source_id>
      </NamedObject>
    </elem>

    <elem>
      <Name>PROPAGATOR</Name>
      <Frequency>1</Frequency>
      <Param>
        <version>8</version>
        <nonRelProp>false</nonRelProp>
        <obsvP>false</obsvP>
        <FermionAction>
          <FermAct>WILSON</FermAct>
          <Kappa>0.11</Kappa>
          <AnisoParam>
            <anisoP>false</anisoP>
            <t_dir>3</t_dir>
            <xi_0>1.0</xi_0>
            <nu>1.0</nu>
          </AnisoParam>
          <FermionBC>
            <FermBC>SIMPLE_FERMBC</FermBC>
            <boundary>1 1 1 0</boundary>
          </FermionBC>
        </FermionAction>
        <InvertParam>
          <invType>CG_INVERTER</invType>
          <RsdCG>1.0e-12</RsdCG>
          <MaxCG>1000</MaxCG>
        </InvertParam>
        <nrow>4 4 4 8</nrow>
      </Param>
      <NamedObject>
        <source_id>sh_source_0</source_id>
        <prop_id>sh_prop_0</prop_id>
      </NamedObject>
    </elem>

    <elem>
      <annotation>
       Mres calculation. If the FermionAction tag is not specified, then the
       action specified in the prop_id file is used.
      </annotation>
      <Name>MRES</Name>
      <Frequency>1</Frequency>
      <Param>
        <version>1</version>
        <FermionAction>
          <FermAct>DWF</FermAct>
          <OverMass>1.4</OverMass>
          <Mass>0.3</Mass>
          <N5>4</N5>
          <FermionBC>
            <FermBC>SIMPLE_FERMBC</FermBC>
            <boundary>1 1 1 0</boundary>
          </FermionBC>
        </FermionAction>
        <InvertParam>
          <invType>CG_INVERTER</invType>
          <RsdCG>1.0e-6</RsdCG>
          <MaxCG>1000</MaxCG>
        </InvertParam>
        <nrow>4 4 4 8</nrow>
      </Param>
      <NamedObject>
        <source_id>sh_source_0</source_id>
        <prop_id>sh_prop_0</prop_id>
      </NamedObject>
    </elem>

  </InlineMeasurements>
  <nrow>4 4 4 8</nrow>
</Param>
<Cfg>
 <cfg_type>SZIN</cfg_type>
 <cfg_file>../../../test_purgaug.cfg1</cfg_file>
</Cfg>
</chroma>


