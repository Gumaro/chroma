        </file_names>
      </File>
      <NamedObject>
        <object_id>eigeninfo_0</object_id>
      </NamedObject>
    </elem>
</annotation>

    <elem>
      <annotation>
        Iterate the smearing operator to produce near eigenvectors
      </annotation>
      <Name>CREATE_COLORVECS</Name>
      <Frequency>1</Frequency>
      <Param>
        <num_vecs>10</num_vecs>
        <decay_dir>3</decay_dir>
        <num_iter>50</num_iter>
        <width>3.0</width>
        <num_orthog>20</num_orthog>
        <LinkSmearing>
          <!-- LinkSmearingType>NONE</LinkSmearingType -->
          <LinkSmearingType>STOUT_SMEAR</LinkSmearingType>
          <link_smear_fact>0.1625</link_smear_fact>
          <link_smear_num>4</link_smear_num>
          <no_smear_dir>3</no_smear_dir>
        </LinkSmearing>
      </Param>
      <NamedObject>
        <gauge_id>default_gauge_field</gauge_id>
        <colorvec_id>eigeninfo_0</colorvec_id>
      </NamedObject>
    </elem>

    <elem>
      <annotation>
        Save this copy
      </annotation>
      <Name>QIO_WRITE_NAMED_OBJECT</Name>
      <Frequency>1</Frequency>
      <NamedObject>
        <object_id>eigeninfo_0</object_id>
        <object_type>SubsetVectorsLatticeColorVector</object_type>
      </NamedObject>
      <File>
        <file_name>./colorvec.lime</file_name>
        <file_volfmt>SINGLEFILE</file_volfmt>
      </File>
    </elem>

    <elem>
      <annotation>
        Compute propagator solution vectors
      </annotation>
      <Name>PROP_COLORVEC</Name>
      <Frequency>1</Frequency>
      <Param>
        <Contractions>
          <num_vecs>10</num_vecs>
          <t_sources>0 3</t_sources>
          <decay_dir>3</decay_dir>
        </Contractions>
        <Propagator>
          <version>10</version>
          <quarkSpinType>FULL</quarkSpinType>
          <obsvP>false</obsvP>
          <numRetries>1</numRetries>
          <FermionAction>
           <FermAct>CLOVER</FermAct>
           <Mass>0.1</Mass>
           <clovCoeff>1.0</clovCoeff>
           <AnisoParam>
             <anisoP>false</anisoP>
           </AnisoParam>
           <FermionBC>
             <FermBC>SIMPLE_FERMBC</FermBC>
             <boundary>1 1 1 -1</boundary>
           </FermionBC>
          </FermionAction>
          <InvertParam>
            <invType>CG_INVERTER</invType>
            <RsdCG>1.0e-8</RsdCG>
            <MaxCG>1000</MaxCG>
          </InvertParam>
        </Propagator>
      </Param>
      <NamedObject>
        <gauge_id>default_gauge_field</gauge_id>
        <colorvec_id>eigeninfo_0</colorvec_id>
        <prop_id>prop_colorvec</prop_id>
      </NamedObject>
    </elem>

    <elem>
      <Name>QIO_WRITE_ERASE_NAMED_OBJECT</Name>
      <Frequency>1</Frequency>
      <NamedObject>
        <object_id>prop_colorvec</object_id>
        <object_type>MapObjectKeyPropColorVecLatticeFermion</object_type>
      </NamedObject>
      <File>
        <file_name>./prop_colorvec.lime</file_name>
        <file_volfmt>MULTIFILE</file_volfmt>
      </File>
    </elem>

    <elem>
      <Name>QIO_READ_NAMED_OBJECT</Name>
      <Frequency>1</Frequency>
