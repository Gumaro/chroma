<?xml version="1.0"?>
<chroma>
<annotation>
Write some QIO objects
</annotation>
<Param> 
  <InlineMeasurements>

    <elem>
      <annotation>
        Snarf/grab gauge arg and stuff into a named object
      </annotation>
      <Name>SNARF_NAMED_OBJECT</Name>
      <Frequency>1</Frequency>
      <NamedObject>
        <object_id>snarf_cfg</object_id>
      </NamedObject>
    </elem>

    <elem>
      <annotation>
        Write the config
      </annotation>
      <Name>QIO_WRITE_NAMED_OBJECT</Name>
      <Frequency>1</Frequency>
      <NamedObject>
        <object_id>snarf_cfg</object_id>
        <object_type>Multi1dLatticeColorMatrix</object_type>
      </NamedObject>
      <File>
        <file_name>qio.cfg</file_name>
        <file_volfmt>MULTIFILE</file_volfmt>
      </File>
    </elem>

    <elem>
      <annotation>
        Read the config back again
      </annotation>
      <Name>QIO_READ_NAMED_OBJECT</Name>
      <Frequency>1</Frequency>
      <NamedObject>
        <object_id>redo_cfg</object_id>
        <object_type>Multi1dLatticeColorMatrix</object_type>
      </NamedObject>
      <File>
        <file_name>qio.cfg</file_name>
      </File>
    </elem>

    <elem>
      <annotation>
        Make a gaussian prop 
      </annotation>
      <Name>GAUSSIAN_INIT_NAMED_OBJECT</Name>
      <Frequency>1</Frequency>
      <NamedObject>
        <object_id>prop_0</object_id>
        <object_type>LatticePropagator</object_type>
      </NamedObject>
    </elem>

    <elem>
      <annotation>
        Write the prop
      </annotation>
      <Name>QIO_WRITE_NAMED_OBJECT</Name>
      <Frequency>1</Frequency>
      <NamedObject>
        <object_id>prop_0</object_id>
        <object_type>LatticePropagator</object_type>
      </NamedObject>
      <File>
        <file_name>./prop_0</file_name>
        <file_volfmt>MULTIFILE</file_volfmt>
      </File>
    </elem>

    <elem>
      <annotation>
        Read the prop
      </annotation>
      <Name>QIO_READ_NAMED_OBJECT</Name>
      <Frequency>1</Frequency>
      <NamedObject>
        <object_id>redo_prop_0</object_id>
        <object_type>LatticePropagator</object_type>
      </NamedObject>
      <File>
        <file_name>./prop_0</file_name>
      </File>
    </elem>

  </InlineMeasurements>
   <nrow>4 4 4 8</nrow>
</Param>
<Cfg>
 <cfg_type>SZIN</cfg_type>
 <cfg_file>../../../test_purgaug.cfg1</cfg_file>
</Cfg>
</chroma>


