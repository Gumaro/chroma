<?xml version="1.0"?>
<chroma>
<annotation>
Write a SZIN config
</annotation>
<Param> 
  <InlineMeasurements>

    <elem>
      <annotation>
        Snarf/grab gauge arg and stuff into a named object
      </annotation>
      <Name>SNARF_NAMED_OBJECT</Name>
      <Frequency>1</Frequency>
      <NamedObject>
        <object_id>snarf_cfg</object_id>
      </NamedObject>
    </elem>

    <elem>
      <Name>SZIN_WRITE_NAMED_OBJECT</Name>
      <Frequency>1</Frequency>
      <NamedObject>
        <object_id>snarf_cfg</object_id>
        <object_type>Multi1dLatticeColorMatrix</object_type>
      </NamedObject>
      <File>
        <file_name>szin.cfg</file_name>
        <trunc>false</trunc>
        <j_decay>3</j_decay>
        <t_start>0</t_start>
        <t_end>7</t_end>
      </File>
    </elem>

    <elem>
      <annotation>
        Make a gaussian prop 
      </annotation>
      <Name>GAUSSIAN_INIT_NAMED_OBJECT</Name>
      <Frequency>1</Frequency>
      <NamedObject>
        <object_id>prop_0</object_id>
        <object_type>LatticePropagator</object_type>
      </NamedObject>
    </elem>

    <elem>
      <annotation>
        List all objects
      </annotation>
      <Name>LIST_NAMED_OBJECT</Name>
      <Frequency>1</Frequency>
    </elem>

    <elem>
      <annotation>
        Write the named object and then erase the memory object
      </annotation>
      <Name>SZIN_WRITE_NAMED_OBJECT</Name>
      <Frequency>1</Frequency>
      <NamedObject>
        <object_id>prop_0</object_id>
        <object_type>LatticePropagator</object_type>
      </NamedObject>
      <File>
        <file_name>./prop_0</file_name>
        <file_volfmt>MULTIFILE</file_volfmt>
      </File>
    </elem>

  </InlineMeasurements>
   <nrow>4 4 4 8</nrow>
</Param>
<Cfg>
 <cfg_type>WEAK_FIELD</cfg_type>
 <cfg_file>dummy</cfg_file>
</Cfg>
</chroma>


