<?xml version="1.0"?>
<params>
  <!-- Lattice Dimensions etc -->
  <lattice>
    <nrow>4 4 4 4</nrow>
    <boundary>1 1 1 -1</boundary>
  </lattice>

  <!-- RNG Seed for initing Gaussians and the like -->
  <RNG>
    <seed>11 0 0 0</seed>
  </RNG>

  <!-- Start up config details -->
  <Cfg>
    <cfg_type>SCIDAC</cfg_type>
    <cfg_file>2flav_cfg_200.lime</cfg_file>
  </Cfg>

  <quarkMass>0.54545454545454545454</quarkMass>
  <eig>
     <RsdR>1.0e-5</RsdR>
     <RsdA>1.0e-6</RsdA>
     <RsdZero>5.0e-6</RsdZero>
     <ProjApsiP>false</ProjApsiP>    
     <Nmin>5</Nmin>
     <Nrenorm>10</Nrenorm>
     <Neig>1</Neig>
     <MaxKS>100</MaxKS>
     <MaxCG>10000</MaxCG>
  </eig>
</params>
